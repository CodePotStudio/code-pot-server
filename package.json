{
	"name": "code-pot-server",
	"version": "1.0.0",
	"description": "code pot back-end server",
	"main": "index.js",
	"scripts": {
		"dev": "concurrently \"sls offline start\" \"yarn generate --watch\"",
		"test": "echo \"Error: no test specified\" && exit 1",
		"deploy:dev": "sls deploy --stage dev",
		"deploy:prod": "sls deploy --stage prod",
		"migrate:local": "dotenv -e .env.local -- npx prisma migrate dev && prisma generate",
		"studio": "dotenv -e .env.local -- npx prisma studio",
		"generate": "graphql-codegen --config codegen.yml",
		"lint": "eslint --fix --ext .js,.jsx,ts,.tsx ."
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@graphql-tools/merge": "^6.2.13",
		"@graphql-tools/schema": "^7.1.4",
		"@prisma/client": "2.21.2",
		"apollo-server-lambda": "^2.24.0",
		"graphql": "^15.5.0",
		"graphql-middleware": "^6.0.9",
		"graphql-scalars": "^1.9.3",
		"graphql-shield": "^7.5.0",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "1.21.4",
		"@graphql-codegen/introspection": "^1.18.2",
		"@graphql-codegen/typescript": "1.22.0",
		"@graphql-codegen/typescript-resolvers": "1.19.1",
		"@types/cookie": "^0.4.0",
		"@types/cookie-parser": "^1.4.2",
		"@types/passport": "^1.0.6",
		"@types/passport-github2": "^1.2.4",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.26.1",
		"@typescript-eslint/parser": "^4.26.1",
		"copy-webpack-plugin": "^8.1.1",
		"dotenv-cli": "^4.0.0",
		"eslint": "^7.28.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-unused-imports": "^1.1.1",
		"prisma": "2.21.2",
		"serverless-domain-manager": "^5.1.0",
		"serverless-dotenv-plugin": "^3.9.0",
